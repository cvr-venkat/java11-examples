node('maven-state') {
    stage ('git') {
        git 'https://github.com/cvr-venkat/java11-examples.git'
    }
    stage ('build') {
        sh ''''
        echo "PATH=${PATH}"
        echo "M2_HOME=${M2_HOME}"
        
        '''
        sh '/usr/local/apache-maven-3.8.4/bin/mvn clean package'
    }
    stage ('archive'){
        archive 'target/*.jar'
    }
}
  
 
